{
  "name": "dryclean-pos-app",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "npx expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "rm -rf node_modules package-lock.json",
    "reinstall": "npm install --legacy-peer-deps",
    "derive": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "podremove": "rm -rf ios/build ios/Pods ios/Podfile.lock",
    "podinstall": "cd ios && pod install --repo-update && cd .."
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.1.9",
    "@aws-amplify/ui-react-native": "^2.5.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native/virtualized-lists": "^0.76.9",
    "@types/react": "~18.3.12",
    "amazon-cognito-identity-js": "^6.3.15",
    "aws-amplify": "^6.14.4",
    "expo": "^52.0.0",
    "expo-permissions": "^14.4.0",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-get-random-values": "^1.11.0",
    "react-native-safe-area-context": "4.12.0",
    "expo-dev-client": "~5.0.20"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.0",
    "@aws-amplify/backend-cli": "^1.7.0",
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-class-static-block": "^7.26.0",
    "@react-native-community/cli": "latest",
    "@types/react": "~18.2.45",
    "aws-cdk-lib": "^2.189.1",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "private": true
}
